<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox prefHeight="620.0" prefWidth="420.0" spacing="5.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.Controller_Rule_Group">
   <children>
      <Label alignment="TOP_LEFT" text="Group name:">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <TextField fx:id="nameField" text="Version X.X.X Updater">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
      <Label text="Description:">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Label>
      <TextArea fx:id="descriptionArea" minHeight="25.0" prefHeight="60.0" wrapText="true">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextArea>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
