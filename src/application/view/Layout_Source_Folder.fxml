<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.Controller_Source_Folder">
   <children>
      <Label alignment="TOP_LEFT" text="Relative path:">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label fx:id="folderPath" alignment="TOP_LEFT" text="C:\build_root\CM_VHL\Cruisecontrol\projects\win-vc10-parallel\scripts\build_root\CM_VHL\Cruisecontrol\projects\win-vc10-parallel\scripts\" wrapText="true">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <CheckBox fx:id="includeMaskCheckBox" mnemonicParsing="false" text="Include only files mask:">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </CheckBox>
      <HBox>
         <children>
            <TextField fx:id="includeMask" disable="true" text="Inherited: empty" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets top="-5.0" />
               </HBox.margin></TextField>
            <Button mnemonicParsing="false" text="Replace all child">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="7.0" />
         </VBox.margin>
      </HBox>
      <CheckBox fx:id="excludeMaskCheckBox" mnemonicParsing="false" text="Exclude files mask:">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </CheckBox>
      <HBox>
         <children>
            <TextField fx:id="excludeMask" disable="true" text="Inherited: *.txt, *.cmd" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets top="-5.0" />
               </HBox.margin></TextField>
            <Button mnemonicParsing="false" text="Replace all child">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
