<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="5.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.Controller_Variable">
   <children>
      <Label text="Variable name:" />
      <TextField fx:id="nameField">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
      <Label text="Value:">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="valueField">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
      <Label text="Description:">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin></Label>
      <TextArea fx:id="descriptionArea" minHeight="25.0" prefHeight="60.0" wrapText="true" />
      <Button mnemonicParsing="false" onAction="#OnButton_VariableRemove" text="Remove Variable">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
